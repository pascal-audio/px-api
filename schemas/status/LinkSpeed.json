  "description": "Network link speed.",
  "oneOf": [
    {
      "description": "10 Mbps",
      "enum": [
        "Speed10"
      ],
      "type": "string"
    },
    {
      "description": "100 Mbps",
      "enum": [
        "Speed100"
      ],
      "type": "string"
    },
    {
